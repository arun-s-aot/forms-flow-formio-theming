<div class="position-relative {{ctx.classes}}" ref="webform">
  <span data-noattach="true" ref="zoomIn" class="formsflow-button formsflow-button--secondary position-absolute" style="right:10px;top:10px;">
    <i class="{{ ctx.iconClass('zoom-in') }}"></i>
  </span>
  <span data-noattach="true" ref="zoomOut" class="formsflow-button formsflow-button--secondary position-absolute" style="right:10px;top:60px;">
    <i class="{{ ctx.iconClass('zoom-out') }}"></i>
  </span>
  <div data-noattach="true" ref="iframeContainer"></div>
  {{ ctx.submitButton }}
</div>
